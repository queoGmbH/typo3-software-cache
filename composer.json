{
  "name": "queo/typo3-software-cache",
  "description": "Software cache for typo3 projects.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "config": {
    "bin-dir": "bin",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2"
    }
  },
  "autoload": {
    "psr-4": {
      "Queo\\Typo3\\SoftwareCache\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Queo\\Typo3\\SoftwareCache\\Tests\\": "tests"
    }
  },
  "require": {
    "beberlei/assert": "*",
    "psr/log": "*",
    "symfony/cache": "*",
    "symfony/http-foundation": "*"
  },
  "require-dev": {
    "phpmetrics/phpmetrics": "~2",
    "phpunit/phpunit": "5.7.*"
  }
}